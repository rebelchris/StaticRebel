{
  "name": "static-rebel-tests",
  "version": "1.0.0",
  "description": "Test suite for Static Rebel AI Assistant",
  "type": "module",
  "scripts": {
    "test": "node --test lib/*.test.js agents/*.test.js dashboard/*.test.js",
    "test:lib": "node --test lib/*.test.js",
    "test:agents": "node --test agents/*.test.js",
    "test:dashboard": "node --test dashboard/*.test.js",
    "test:config": "node --test lib/configManager.test.js",
    "test:memory": "node --test lib/memoryManager.test.js",
    "test:models": "node --test lib/modelRegistry.test.js",
    "test:subagents": "node --test lib/subagentManager.test.js",
    "test:cron": "node --test lib/cronScheduler.test.js",
    "test:watch": "node --watch --test lib/*.test.js agents/*.test.js dashboard/*.test.js"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
